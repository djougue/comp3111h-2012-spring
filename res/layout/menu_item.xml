<?xml version="1.0" encoding="utf-8"?>

<LinearLayout 
      xmlns:android="http://schemas.android.com/apk/res/android"
      android:layout_height="wrap_content"
      android:layout_width="fill_parent"
      android:orientation="vertical"
      android:id="@+id/parentLayout"
    >
    <FrameLayout 
      android:layout_height="wrap_content"
      android:layout_width="fill_parent"
    >
    
	    <ImageView
	        android:id="@+id/tl"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="@dimen/framesize"
	        android:scaleType="fitXY" 
	        android:src="@drawable/tl"
	        android:layout_gravity="left"/>
	    <ImageView
	        android:id="@+id/tr"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="@dimen/framesize"
	        android:scaleType="fitXY" 
	        android:src="@drawable/tr"
	        android:layout_gravity="right"/>
	    <ImageView 
	        android:id="@+id/tm"
	        android:layout_width="fill_parent"
	        android:layout_marginLeft="@dimen/framesize"
	        android:layout_marginRight="@dimen/framesize"
	        android:layout_height="@dimen/framesize"
	        android:src="@drawable/tm"
	        android:scaleType="fitXY" 
	        android:layout_gravity="center_horizontal" />"
	</FrameLayout>
    <FrameLayout 
      android:layout_height="wrap_content"
      android:layout_width="fill_parent"
    >
	    <ImageView
	        android:id="@+id/ml"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="50dip"
	        android:src="@drawable/ml"
	        android:scaleType="fitXY" 
	        android:layout_gravity="left"/>
		<ImageView
		    android:id="@+id/mr"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="50dip"
	        android:src="@drawable/mr"
	        android:scaleType="fitXY" 
	        android:layout_gravity="right"
	        />
	    	        
		<LinearLayout
		    android:id="@+id/mm"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:background="#FFFFFF"
		    android:layout_marginLeft="@dimen/framesize"
		    android:layout_marginRight="@dimen/framesize"
		    android:layout_gravity="center_horizontal"
		  >
		  <ImageView
		  	  android:id="@+id/image"
			  android:layout_width="50dip"
			  android:layout_height="50dip" android:src="@drawable/stub" android:scaleType="centerCrop"/>
		  <FrameLayout 
		      android:layout_width="fill_parent"
		      android:layout_height="fill_parent">
		   	  <TextView
			  	  android:id="@+id/text1"
			  	  android:layout_width="80dip"
				  android:layout_height="34dip"
				  android:layout_gravity="bottom|right"
				  android:textSize="30dip" 
				  android:textStyle="bold"
				  android:text = "HelloWorld"
			  />"
			  <LinearLayout
			      xmlns:android="http://schemas.android.com/apk/res/android"
			      android:layout_width="fill_parent"
			      android:layout_height="wrap_content"
			      android:layout_gravity="top|left"
			      android:paddingLeft="2dip"
			      android:paddingTop="2dip"
			      android:paddingRight="2dip"
			      android:orientation="vertical">	
			
				  <TextView
				  	  android:id="@+id/text2"
				  	  android:layout_width="fill_parent"
					  android:layout_height="16dip"
					  android:layout_gravity="top" android:textSize="12dip" android:layout_marginLeft="10dip"
					  android:textStyle="bold"
					  android:text="for_debug_purpose"/>
				  <TextView
				  	  android:id="@+id/text3"
				  	  android:layout_width="fill_parent"
					  android:layout_height="15dip"
					  android:layout_gravity="top" android:textSize="12dip" android:layout_marginLeft="10dip"
					  android:text="for_debug_purpose"/>
				  <!-- A block for food style image -->
				  <LinearLayout
				      android:orientation="horizontal" 
				      android:layout_height="wrap_content"
				      android:layout_width="wrap_content"
				      android:layout_marginLeft="10dip"
				      android:id="@+id/imagebox"
				      >
				      <ImageView 
				          android:id="@+id/iconSpicy"
				          android:layout_width="@dimen/iconsize"
				          android:layout_height="@dimen/iconsize"
				          android:src="@drawable/icon_spicy"
				          android:layout_marginRight="5dip"
				          android:scaleType="fitXY"
				          />
				      <ImageView 
				          android:id="@+id/iconVega"
				          android:layout_width="@dimen/iconsize"
				          android:layout_height="@dimen/iconsize"
				          android:src="@drawable/icon_vega"
				          android:layout_marginRight="5dip"
				          android:scaleType="fitXY"
				          />
				      <ImageView 
				          android:id="@+id/iconMeat"
				          android:layout_width="@dimen/iconsize"
				          android:layout_height="@dimen/iconsize"
				          android:src="@drawable/icon_meat"
				          android:layout_marginRight="5dip"
				          android:scaleType="fitXY"
				          />
				  </LinearLayout>
				  	  
			  </LinearLayout>  
		</FrameLayout>
		<!--    <LinearLayout 
		      xmlns:android="http://schemas.android.com/apk/res/android"
		      android:layout_width="fill_parent"
		      android:layout_height="fill_parent"
		      android:layout_weight="3">
		 	  <TextView
			  	  android:id="@+id/priceText"
			  	  android:layout_width="fill_parent"
				  android:layout_height="wrap_content"
				  android:layout_gravity="left|center_vertical" 
				  android:textSize="30dip" 
				  android:layout_marginLeft="10dip"
				  android:textStyle="bold|italic"
				  />
		      
		  </LinearLayout>-->
			
		</LinearLayout>
		
	</FrameLayout>
    <FrameLayout 
      android:layout_height="wrap_content"
      android:layout_width="fill_parent"
    >
    
	    <ImageView 
	        android:id="@+id/bl"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="@dimen/framesize"
	        android:scaleType="fitXY" 
	        android:src="@drawable/bl"
	        android:layout_gravity="left"/>
	    <ImageView 
	        android:id="@+id/br"
	        android:layout_width="@dimen/framesize"
	        android:layout_height="@dimen/framesize"
	        android:scaleType="fitXY" 
	        android:src="@drawable/br"
	        android:layout_gravity="right"/>	    
	    	    
	    <ImageView 
	        android:id="@+id/bm"
	        android:layout_width="fill_parent"
	        android:layout_height="@dimen/framesize"
	        android:src="@drawable/bm"
	        android:layout_marginRight="@dimen/framesize"
	        android:layout_marginLeft="@dimen/framesize"
	        android:scaleType="fitXY" 
	        android:layout_gravity="center_horizontal" />"
	</FrameLayout>
</LinearLayout>
